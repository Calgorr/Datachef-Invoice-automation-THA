{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "Statement1",
			"Effect": "Allow",
			"Action": [
				"s3:PutObject",
				"s3:DeleteObject",
				"s3:GetObject",
				"s3:GetObjectAcl",
				"s3:PutObjectAcl",
				"s3:GetObjectTagging",
				"s3:DeleteObjectTagging",
				"s3:PutObjectTagging",
				"s3:ListBucket",
				"s3:ListTagsForResource"
			],
			"Resource": [
				"arn:aws:s3:::dc-tha5-source-bucket",
				"arn:aws:s3:::dc-tha5-source-bucket/*",
				"arn:aws:s3:::dc-tha5-destination-bucket",
				"arn:aws:s3:::dc-tha5-destination-bucket/*"
			]
		}
	]
}